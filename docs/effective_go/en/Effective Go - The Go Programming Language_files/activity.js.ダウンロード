CommonUtils.encodeURL=function(str){var c="";var uc="";var s="";for(var i=0;i<str.length;i++){c=str.charAt(i);uc=str.charCodeAt(i);if(uc==42||uc==45||uc==46||uc==95||uc>=48&&uc<=57||uc>=65&&uc<=90||uc>=97&&uc<=122)s+=c;else if(uc==32)s+="+";else if(uc>=0&&uc<=127){c="0"+uc.toString(16);s+="%"+c.substr(c.length-2)}else if(uc>2097151){s+="%"+(240+((uc&1835008)>>18)).toString(16);s+="%"+(128+((uc&258048)>>12)).toString(16);s+="%"+(128+((uc&4032)>>6)).toString(16);s+="%"+(128+(uc&63)).toString(16)}else if(uc>2047){s+=
"%"+(224+((uc&61440)>>12)).toString(16);s+="%"+(128+((uc&4032)>>6)).toString(16);s+="%"+(128+(uc&63)).toString(16)}else{s+="%"+(192+((uc&1984)>>6)).toString(16);s+="%"+(128+(uc&63)).toString(16)}}return s};
